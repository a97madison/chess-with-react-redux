import {ActionBase} from "actions/ActionBase";

/**
 * Toggle history action
 */
export public class ToggleHistoryAction extends ActionBase {
	public isHistory: boolean;

	public constructor(@Spec spec:~i~this) {}
}

export public function createToggleHistoryAction(isHistory: boolean): ActionBase {
	console.log("toggle history action");
	return new ToggleHistoryAction({ isHistory : isHistory });
}
